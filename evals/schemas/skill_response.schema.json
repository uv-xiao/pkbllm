{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Skill eval run response",
  "type": "object",
  "properties": {
    "invoked_skills": {
      "type": "array",
      "items": { "type": "string" }
    },
    "summary": { "type": "string" },
    "questions": {
      "type": "array",
      "items": { "type": "string" }
    },
    "steps": {
      "type": "array",
      "items": { "type": "string" }
    },
    "verifications": {
      "type": "array",
      "items": { "type": "string" }
    },
    "artifacts": {
      "type": "array",
      "items": { "type": "string" }
    }
  },
  "required": ["invoked_skills", "summary", "questions", "steps", "verifications", "artifacts"],
  "additionalProperties": false
}
